{% extends 'base.jinja2' %}

{% block content %}
  <header class="w3-container w3-brown">
    <h2>{{ page_title|capitalize }}</h2>
  </header>

  <div class="w3-container w3-panel w3-white">
    <h3>Add new</h3>
    <p>
      {% for c in range(50,950,50) %}
        <a style="line-height: 1; margin: 5px; margin-bottom: 10px;" class="w3-button w3-deep-orange" href="/add/{{ c }}">{{ c }}</a>
      {% endfor %}
    </p>

    <p>
      <a class="w3-button w3-blue" href={{ url_for('index') }}><i class="fa fa-fw fa-arrow-left"></i> Index</a>
    </p>

  </div>

{% endblock %}
